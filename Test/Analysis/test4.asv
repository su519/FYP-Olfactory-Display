close all

x1 = Lemon(:, 2); % Second column as x-values
y1 = Lemon(:, 4); % Fourth column as y-values

le = '#FFF86C';
la = '#c69aff';
o = '#fdbb5e';
i = '#c0dfff';
lg = '#d2f78c';
s = '#FFC3F5';

figure;

scatter(x1(1), y1(1), 200,'filled', "MarkerFaceColor", '#c69aff');
hold on
grid on
scatter(x1(2:7), y1(2:7), 200, 'filled', "MarkerFaceColor", '#fdbb5e');
scatter(x1(8:11), y1(8:11), 200, 'filled', "MarkerFaceColor", '#c0dfff');
scatter(x1(12:14), y1(12:14), 200, 'filled', "MarkerFaceColor", '#d2f78c');
scatter(x1(15), y1(15), 200, 'filled', "MarkerFaceColor",'#FFC3F5');
xlim([0,1]);
ylim([0,1]);
xlabel('Lemon Scent Intensity','FontSize',12);
ylabel('Other Scent Intensity','FontSize',12);
title('Lemon','FontSize',14);
legend('Lavender','Orange','Iris','Lemongrass','Sakura','Location','Best');

